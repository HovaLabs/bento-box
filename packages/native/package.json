  "author": "Kaitlyn Hova & Matt Hova",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublish": "rm -rf dist && npm run build",
    "typecheck": "tsc --noEmit",
