    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-native": "^0.61",
    "react-native-dark-mode": "^0.2.1",
    "react-scripts": "^3.3.0",
    "typescript": "^3.7.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/plugin-proposal-numeric-separator": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@lerna/publish": "^3.18.5",
